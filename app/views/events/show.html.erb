  <%=h @event.logo_path %>
</p>


<script type="text/javascript">
  options = {
    showOn: 'button',
    buttonImage: '/images/icons/calendar.png',
    buttonImageOnly: true,
    dateFormat: 'yy-mm-dd',
    constrainInput: true,
    minDate: new Date()
  }
  // datepicker start_date
  $jq(function() {
    $jq("#start_date").datepicker(options);
  });

  $jq(function() {
    $jq("#end_date").datepicker(options);
  });

</script>
